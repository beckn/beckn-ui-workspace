{
  "$comment": "This file defines HTML templates and rendering hints for client-side display of EV charging service data. It specifies how to render discovery listings, charging station cards, and detail views.",
  "description": "Renderer configuration for EV Charging Service UI components - defines HTML templates and rendering hints for client-side display",
  "version": "1.0.0",
  "templates": {
    "discoveryCard": {
      "name": "EV Charging Service Discovery Card",
      "description": "Rich card view for EV charging stations in discovery/search results",
      "html": "<div class=\"item-card\" data-item-id=\"{{beckn:id}}\">\n  <div class=\"item-main\">\n    <div class=\"item-header\">\n      <h3 class=\"item-name\">{{beckn:descriptor.schema:name}}</h3>\n      <span class=\"item-badge badge-speed speed-{{beckn:itemAttributes.chargingSpeed}}\">{{beckn:itemAttributes.chargingSpeed}}</span>\n    </div>\n    <p class=\"item-short-desc\">{{beckn:descriptor.beckn:shortDesc}}</p>\n    <div class=\"item-location\">\n      <span class=\"location-icon\">üìç</span>\n      <span class=\"location-text\">{{beckn:itemAttributes.chargingStation.serviceLocation.address.streetAddress}}, {{beckn:itemAttributes.chargingStation.serviceLocation.address.addressLocality}}</span>\n    </div>\n    {{#if beckn:itemAttributes.connectorType}}\n    <div class=\"item-spec-summary\">\n      <div class=\"item-attribute-row\"><span class=\"item-attribute-chip\">Connector: {{beckn:itemAttributes.connectorType}}</span></div>\n      <div class=\"item-attribute-row\"><span class=\"item-attribute-chip\">Power: {{beckn:itemAttributes.maxPowerKW}}kW</span></div>\n      <div class=\"item-attribute-row\"><span class=\"item-attribute-chip\">Type: {{beckn:itemAttributes.powerType}}</span></div>\n    </div>\n    {{/if}}\n    <div class=\"item-price-row\">\n      <div class=\"price-chip\">\n        <span class=\"price-currency\">{{beckn:offers[0].beckn:price.currency}}</span>\n        <span class=\"price-value\">{{beckn:offers[0].beckn:price.value}}</span>\n        <span class=\"price-unit\">/ {{beckn:offers[0].beckn:price.applicableQuantity.unitText}}</span>\n      </div>\n    </div>\n  </div>\n</div>",
      "dataPaths": {
        "itemId": "beckn:id",
        "itemName": "beckn:descriptor.schema:name",
        "shortDescription": "beckn:descriptor.beckn:shortDesc",
        "longDescription": "beckn:descriptor.beckn:longDesc",
        "connectorType": "beckn:itemAttributes.connectorType",
        "maxPowerKW": "beckn:itemAttributes.maxPowerKW",
        "minPowerKW": "beckn:itemAttributes.minPowerKW",
        "socketCount": "beckn:itemAttributes.socketCount",
        "chargingSpeed": "beckn:itemAttributes.chargingSpeed",
        "powerType": "beckn:itemAttributes.powerType",
        "connectorFormat": "beckn:itemAttributes.connectorFormat",
        "stationStatus": "beckn:itemAttributes.stationStatus",
        "reservationSupported": "beckn:itemAttributes.reservationSupported",
        "parkingType": "beckn:itemAttributes.parkingType",
        "amenityFeature": "beckn:itemAttributes.amenityFeature",
        "serviceLocation": "beckn:itemAttributes.chargingStation.serviceLocation",
        "streetAddress": "beckn:itemAttributes.chargingStation.serviceLocation.address.streetAddress",
        "addressLocality": "beckn:itemAttributes.chargingStation.serviceLocation.address.addressLocality",
        "addressRegion": "beckn:itemAttributes.chargingStation.serviceLocation.address.addressRegion",
        "postalCode": "beckn:itemAttributes.chargingStation.serviceLocation.address.postalCode",
        "geoCoordinates": "beckn:itemAttributes.chargingStation.serviceLocation.geo.coordinates",
        "ocppId": "beckn:itemAttributes.ocppId",
        "evseId": "beckn:itemAttributes.evseId",
        "roamingNetwork": "beckn:itemAttributes.roamingNetwork",
        "connectorId": "beckn:itemAttributes.connectorId",
        "ratingValue": "beckn:rating.beckn:ratingValue",
        "ratingCount": "beckn:rating.beckn:ratingCount",
        "price": "beckn:offers[0].beckn:price.value",
        "currency": "beckn:offers[0].beckn:price.currency",
        "priceUnit": "beckn:offers[0].beckn:price.applicableQuantity.unitText"
      }
    },
    "detailView": {
      "name": "EV Charging Service Detail View",
      "description": "Comprehensive detail view for a single EV charging station",
      "html": "<div class=\"item-detail\" data-item-id=\"{{beckn:id}}\">\n  <div class=\"item-header\">\n    <h1 class=\"item-name\">{{beckn:descriptor.schema:name}}</h1>\n    <span class=\"item-badge badge-speed speed-{{beckn:itemAttributes.chargingSpeed}}\">{{beckn:itemAttributes.chargingSpeed}}</span>\n  </div>\n  <p class=\"item-long-desc\">{{beckn:descriptor.beckn:longDesc}}</p>\n  <div class=\"item-specifications\">\n    <h2>Specifications</h2>\n    <div class=\"spec-row\"><span class=\"spec-label\">Connector Type: </span><span class=\"spec-value\">{{beckn:itemAttributes.connectorType}}</span></div>\n    <div class=\"spec-row\"><span class=\"spec-label\">Power Range: </span><span class=\"spec-value\">{{beckn:itemAttributes.minPowerKW}}kW - {{beckn:itemAttributes.maxPowerKW}}kW</span></div>\n    <div class=\"spec-row\"><span class=\"spec-label\">Charging Speed: </span><span class=\"spec-value\">{{beckn:itemAttributes.chargingSpeed}}</span></div>\n    <div class=\"spec-row\"><span class=\"spec-label\">Power Type: </span><span class=\"spec-value\">{{beckn:itemAttributes.powerType}}</span></div>\n    <div class=\"spec-row\"><span class=\"spec-label\">Connector Format: </span><span class=\"spec-value\">{{beckn:itemAttributes.connectorFormat}}</span></div>\n  </div>\n  <div class=\"item-location-info\">\n    <h2>Location</h2>\n    <div class=\"location-row\"><span class=\"location-label\">Address: </span><span class=\"location-value\">{{beckn:itemAttributes.chargingStation.serviceLocation.address.streetAddress}}</span></div>\n    <div class=\"location-row\"><span class=\"location-label\">City: </span><span class=\"location-value\">{{beckn:itemAttributes.chargingStation.serviceLocation.address.addressLocality}}</span></div>\n    <div class=\"location-row\"><span class=\"location-label\">State: </span><span class=\"location-value\">{{beckn:itemAttributes.chargingStation.serviceLocation.address.addressRegion}}</span></div>\n    <div class=\"location-row\"><span class=\"location-label\">Postal Code: </span><span class=\"location-value\">{{beckn:itemAttributes.chargingStation.serviceLocation.address.postalCode}}</span></div>\n  </div>\n  {{#if beckn:itemAttributes.amenityFeature}}\n  <div class=\"item-amenities\">\n    <h2>Amenities</h2>\n    {{#each beckn:itemAttributes.amenityFeature}}\n    <div class=\"amenity-row\"><span class=\"amenity-name\">{{this}}</span><span class=\"amenity-value\">‚úì</span></div>\n    {{/each}}\n  </div>\n  {{/if}}\n  <div class=\"item-rating\">\n    <span class=\"rating-value\">{{beckn:rating.beckn:ratingValue}}</span>\n    <span class=\"rating-stars\">‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ</span>\n    <span class=\"rating-count\">({{beckn:rating.beckn:ratingCount}} reviews)</span>\n  </div>\n  <div class=\"item-pricing\">\n    <span class=\"price-currency\">{{beckn:offers[0].beckn:price.currency}}</span>\n    <span class=\"price-value\">{{beckn:offers[0].beckn:price.value}}</span>\n    <span class=\"price-unit\">per {{beckn:offers[0].beckn:price.applicableQuantity.unitText}}</span>\n  </div>\n</div>",
      "dataPaths": {
        "itemId": "beckn:id",
        "itemName": "beckn:descriptor.schema:name",
        "longDescription": "beckn:descriptor.beckn:longDesc",
        "connectorType": "beckn:itemAttributes.connectorType",
        "maxPowerKW": "beckn:itemAttributes.maxPowerKW",
        "minPowerKW": "beckn:itemAttributes.minPowerKW",
        "socketCount": "beckn:itemAttributes.socketCount",
        "chargingSpeed": "beckn:itemAttributes.chargingSpeed",
        "powerType": "beckn:itemAttributes.powerType",
        "connectorFormat": "beckn:itemAttributes.connectorFormat",
        "stationStatus": "beckn:itemAttributes.stationStatus",
        "reservationSupported": "beckn:itemAttributes.reservationSupported",
        "parkingType": "beckn:itemAttributes.parkingType",
        "amenityFeature": "beckn:itemAttributes.amenityFeature",
        "serviceLocation": "beckn:itemAttributes.chargingStation.serviceLocation",
        "streetAddress": "beckn:itemAttributes.chargingStation.serviceLocation.address.streetAddress",
        "addressLocality": "beckn:itemAttributes.chargingStation.serviceLocation.address.addressLocality",
        "addressRegion": "beckn:itemAttributes.chargingStation.serviceLocation.address.addressRegion",
        "postalCode": "beckn:itemAttributes.chargingStation.serviceLocation.address.postalCode",
        "geoCoordinates": "beckn:itemAttributes.chargingStation.serviceLocation.geo.coordinates",
        "ocppId": "beckn:itemAttributes.ocppId",
        "evseId": "beckn:itemAttributes.evseId",
        "roamingNetwork": "beckn:itemAttributes.roamingNetwork",
        "connectorId": "beckn:itemAttributes.connectorId",
        "ratingValue": "beckn:rating.beckn:ratingValue",
        "ratingCount": "beckn:rating.beckn:ratingCount",
        "price": "beckn:offers[0].beckn:price.value",
        "currency": "beckn:offers[0].beckn:price.currency",
        "priceUnit": "beckn:offers[0].beckn:price.applicableQuantity.unitText"
      }
    },
    "compactCard": {
      "name": "EV Charging Service Compact Card",
      "description": "Minimal card view for grid/list layouts",
      "html": "<div class=\"item-card item-compact\" data-item-id=\"{{beckn:id}}\">\n  <div class=\"item-header\">\n    <h4 class=\"item-name\">{{beckn:descriptor.schema:name}}</h4>\n    <span class=\"item-badge badge-speed speed-{{beckn:itemAttributes.chargingSpeed}}\">{{beckn:itemAttributes.chargingSpeed}}</span>\n  </div>\n  <div class=\"item-location\">{{beckn:itemAttributes.chargingStation.serviceLocation.address.addressLocality}}</div>\n  <div class=\"item-spec-summary\">\n    <span class=\"item-attribute-chip\">{{beckn:itemAttributes.connectorType}}</span>\n    <span class=\"item-attribute-chip\">{{beckn:itemAttributes.maxPowerKW}}kW</span>\n    <span class=\"item-attribute-chip\">{{beckn:itemAttributes.socketCount}} sockets</span>\n  </div>\n  <div class=\"item-status status-{{beckn:itemAttributes.stationStatus}}\">{{beckn:itemAttributes.stationStatus}}</div>\n  <div class=\"item-price\">{{beckn:offers[0].beckn:price.currency}} {{beckn:offers[0].beckn:price.value}}/{{beckn:offers[0].beckn:price.applicableQuantity.unitText}}</div>\n</div>",
      "dataPaths": {
        "itemId": "beckn:id",
        "itemName": "beckn:descriptor.schema:name",
        "chargingSpeed": "beckn:itemAttributes.chargingSpeed",
        "connectorType": "beckn:itemAttributes.connectorType",
        "maxPowerKW": "beckn:itemAttributes.maxPowerKW",
        "socketCount": "beckn:itemAttributes.socketCount",
        "stationStatus": "beckn:itemAttributes.stationStatus",
        "addressLocality": "beckn:itemAttributes.chargingStation.serviceLocation.address.addressLocality",
        "price": "beckn:offers[0].beckn:price.value",
        "currency": "beckn:offers[0].beckn:price.currency",
        "priceUnit": "beckn:offers[0].beckn:price.applicableQuantity.unitText"
      }
    }
  },
  "stylingHints": {
    "chargingSpeedBadge": {
