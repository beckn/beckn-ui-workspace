{
  "name": "beckn-ui-workspace",
  "packageManager": "yarn@3.6.3",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/deg-ev-charging"
  ],
  "prettier": "@beckn-ui/prettier-config",
  "scripts": {
    "postinstall": "yarn prepare",
    "prepare": "npx husky install",
    "build:common": "yarn workspace @beckn-ui/common prepare",
    "dev:deg-ev-charging": "yarn workspace @beckn-ui/deg-ev-charging dev",
    "start:deg-ev-charging": "yarn workspace @beckn-ui/deg-ev-charging start",
    "build:deg-ev-charging": "yarn workspace @beckn-ui/deg-ev-charging build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest",
    "lint:fix": "eslint --fix",
    "lint": "eslint",
    "format": "prettier  \"./**/*.{js,jsx,ts,tsx,json}\" --write"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn format"
    ]
  },
  "dependencies": {
    "@beckn-ui/becknified-components": "workspace:^",
    "@beckn-ui/common": "workspace:^",
    "@beckn-ui/molecules": "workspace:^",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@react-google-maps/api": "^2.19.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/google-maps": "^3.2.6",
    "@types/google.maps": "^3.55.12",
    "@types/googlemaps": "^3.43.3",
    "@types/mongoose": "^5.11.97",
    "axios": "^1.6.2",
    "chakra-ui": "^0.3.8",
    "eslint-config-next": "latest",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "framer-motion": "^10.16.4",
    "mongodb": "^6.2.0",
    "mongoose": "^8.0.0",
    "next": "^13.5.4",
    "qrcode.react": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-places-autocomplete": "^7.3.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "redux-persist": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@beckn-ui/prettier-config": "workspace:^",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "cypress": "latest",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
